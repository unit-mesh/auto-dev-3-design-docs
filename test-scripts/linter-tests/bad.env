# Bad .env file for testing dotenv-linter

# Duplicate key
API_KEY=first_value
API_KEY=second_value

# Unordered keys
ZEBRA_KEY=value
APPLE_KEY=value

# Lowercase key
database_url=postgres://localhost

# Space around equals
API_TOKEN = mytoken

# Leading/trailing whitespace
SECRET_KEY= my_secret 

# Empty value
EMPTY_VAR=

# Incorrect quote usage
QUOTED_VALUE="value with spaces"

